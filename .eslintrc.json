{
  "parser": "@babel/eslint-parser",
  "ecmaFeatures": {
    "modules": true,
    "spread": true,
    "restParams": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2020,
    "sourceType": "module"
  }
}
